<Window x:Class="element_profiles.Controls.AddEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:element_profiles.Controls"
        mc:Ignorable="d"
        ThemeMode="System"
        Title="Add Entry" Height="130" Width="344" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="TextBoxProfileName" Margin="109,11,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="27"/>
        <Label Content="Profile Name: " HorizontalAlignment="Left" Margin="10,17,0,0" VerticalAlignment="Top" Width="94"/>
        <Button x:Name="ButtonOk" Click="ButtonOk_Click" Content="Create" Margin="0,0,81,10" VerticalAlignment="Bottom" Height="32" HorizontalAlignment="Right" Width="66"/>
        <Button x:Name="ButtonCancel" Click="ButtonCancel_Click" Content="Cancel" Margin="0,0,10,10" VerticalAlignment="Bottom" Height="32" HorizontalAlignment="Right" Width="66"/>

    </Grid>
</Window>
