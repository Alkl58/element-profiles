<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <RootNamespace>element_profiles</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Assets\app.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\app.ico" />
    <None Remove="Assets\icons8-add-96.png" />
    <None Remove="Assets\icons8-remove-96.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\app.ico" />
    <Resource Include="Assets\icons8-add-96.png" />
    <Resource Include="Assets\icons8-remove-96.png" />
  </ItemGroup>
</Project>
